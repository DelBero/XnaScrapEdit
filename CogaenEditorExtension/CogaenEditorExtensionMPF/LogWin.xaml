<UserControl x:Class="CogaenEditExtension.LogWin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             x:Name="LogCtrl"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" xmlns:my="clr-namespace:CogaenEditExtension">
    <Grid>
        <ListBox ItemsSource="{Binding ElementName=LogCtrl, Path=Log.Messages}"></ListBox>
    </Grid>
</UserControl>
